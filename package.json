  "private": false,
  "description": "Professional-grade AI-powered cryptocurrency trading platform with autonomous trading capabilities, advanced risk management, and real-time market analysis using Claude Sonnet 4 AI and Delta Exchange API",
  "keywords": [
    "cryptocurrency",
    "trading",
    "ai",
    "algorithmic-trading",
    "delta-exchange",
    "claude-ai",
    "risk-management",
    "portfolio-optimization",
    "autonomous-trading",
    "nextjs",
    "typescript",
    "websocket",
    "real-time",
    "fintech"
  ],
  "homepage": "https://v0-crypto-trading-website-ashy.vercel.app",
  "bugs": {
    "url": "https://github.com/vishwamartur/crypto-trading-website/issues"
